{
  "compilerOptions": {
    "moduleResolution": "node",
    "outDir": "./dist/",
    "target": "ES2015",
    "jsx": "react"
  },
  "allowSyntheticDefaultImports": true,
  "allowJs": true /*JavaScriptのコンパイルには必須*/,
  "checkJs": true /*JavaScriptで簡易的な型チェックを行う*/,
  "sourceMap": true,
  "inlineSources": true,
  "outDir": "../../dist",
  "strict": true
}
